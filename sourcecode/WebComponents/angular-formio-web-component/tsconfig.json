{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "types": [
      "gapi",
      "gapi.auth2",
      "gapi.client",
      "gapi.client.drive",
      "gapi.client.sheets",
      "google.picker"
    ],
    "sourceMap": false,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "target": "es2015",
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "es2015",
      "dom"
    ],
    "paths": {
      "module-components": [
        "dist/module-components"
      ],
      "module-components/*": [
        "dist/module-components/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": false
  },
}