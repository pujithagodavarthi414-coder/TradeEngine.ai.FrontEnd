{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "4b07efce-7a04-408a-880d-4bfb17bd6010"
  },
  "version": 1,
  "newProjectRoot": "app-builder-module-components",
  "projects": {    
    "launcher.dev": {
      "root": "app-builder-module-components/launcher.dev/",
      "sourceRoot": "app-builder-module-components/launcher.dev/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/launcher.dev",
            "index": "app-builder-module-components/launcher.dev/src/index.html",
            "main": "app-builder-module-components/launcher.dev/src/main.ts",
            "polyfills": "app-builder-module-components/launcher.dev/src/polyfills.ts",
            "tsConfig": "app-builder-module-components/launcher.dev/tsconfig.app.json",
            "assets": [
              "app-builder-module-components/launcher.dev/src/favicon.ico",
              "app-builder-module-components/launcher.dev/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/bpmn-js/dist/assets/diagram-js.css",
              "node_modules/bpmn-js/dist/assets/bpmn-font/css/bpmn-embedded.css",
              "node_modules/bpmn-js-properties-panel/styles/properties.less",
              "node_modules/dmn-js/dist/assets/diagram-js.css",
              "node_modules/bpmn-js-properties-panel/dist/assets/bpmn-js-properties-panel.css",
              "node_modules/dmn-js/dist/assets/dmn-js-decision-table-controls.css",
              "node_modules/dmn-js/dist/assets/dmn-js-decision-table.css",
              "node_modules/dmn-js/dist/assets/dmn-js-drd.css",
              "node_modules/dmn-js/dist/assets/dmn-js-literal-expression.css",
              "node_modules/dmn-js/dist/assets/dmn-js-shared.css",
              "node_modules/dmn-js/dist/assets/dmn-font/css/dmn.css",
              "app-builder-module-components/launcher.dev/src/assets/styles/app.scss",
              "node_modules/intro.js/introjs.css",
              "app-builder-module-components/launcher.dev/src/styles.css"
            ],
            "scripts": [
              "app-builder-module-components/app-builder-components/src/create-form/formio-web-components.js",
              "app-builder-module-components/app-builder-components/src/Html-designer/pdf-designer-webcomponent.js",
              "app-builder-module-components/app-builder-components/src/workflows/workflow-web-components.js",
              
              {
                "input": "node_modules/document-register-element/build/document-register-element.js",
                "inject": true
              },
              "node_modules/intro.js/intro.js",
              "node_modules/jquery/dist/jquery.min.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "app-builder-module-components/launcher.dev/src/environments/environment.ts",
                  "with": "app-builder-module-components/launcher.dev/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "50mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "launcher.dev:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "launcher.dev:build:production"
            }
          }
        }
      }
    },
    "project-components": {
      "root": "app-builder-module-components/app-builder-components",
      "sourceRoot": "app-builder-module-components/app-builder-components/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "app-builder-module-components/app-builder-components/tsconfig.lib.json",
            "project": "app-builder-module-components/app-builder-components/ng-package.json"
          }
        ,          "configurations": {
            "production": {
              "tsConfig": "app-builder-module-components/app-builder-components/tsconfig.lib.prod.json"
            }
          }
}
      }
    }
  },
  "defaultProject": "launcher.dev"
}