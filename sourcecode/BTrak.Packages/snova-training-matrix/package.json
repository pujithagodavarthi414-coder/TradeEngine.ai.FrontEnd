  "scripts": {
    "launch-dev": "node --max_old_space_size=12000 ./node_modules/@angular/cli/bin/ng serve --project=launcher.dev -o --port=4203",
    "build-dev": "ng build --configuration production --project=launcher.dev",
    "launch-package": "ng serve --project=launcher.package -o --port=4201",
    "build-package": "ng build --configuration production --project=launcher.package",
    "build": "ng build project-components",
